{
  "name": "clever-typescript",
  "version": "2.1.0",
  "description": "Clever API generated with OpenAPI Generator",
  "repository": "https://github.com/dogomedia/clever-typescript",
  "keywords": [
    "clever",
    "api",
    "typescript",
    "library"
  ],
  "dependencies": {
    "axios": "^0.19.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.5.2",
    "@types/axios": "^0.14.0",
    "@types/node": "^13.11.1",
    "gts": "^2.0.2",
    "typescript": "^3.8.3"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "regenerate": "npx openapi-generator-cli generate -g typescript-axios -i https://raw.githubusercontent.com/Clever/swagger-api/master/full-v2.yml -o ./src",
    "build": "tsc",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
